/** QToolBar **/

QWidget#JChartView QToolBar {
    background: #323232;
    spacing: 2px;
    min-height: 20px;
    padding: 0;
}

QWidget#JChartView QToolBar::separator {
    width: 1px;
    background: #555;
}

QWidget#JChartView QToolBar QToolButton {
    background: transparent;
    border: 0;
}

QWidget#JChartView QToolBar QToolButton:hover {
    background: #445;
    border: 0;
}

QWidget#JChartView QToolBar QToolButton:pressed {
    background: #446;
    border: 0;
}

/** QTreeView QLineEdit **/

QTreeView QLineEdit {
    border-radius: 0;
    padding: 0;
    border: 0;
}

QTreeView  QLineEdit:hover {
    border: 0;
}

QTreeView QLineEdit:focus:!read-only {
    border-radius: 0;
}

/**********************************************************/

/** QToolButton#buttonClose **/

QToolButton#buttonClose {
    background: #20383838;
    padding: 1px;
    border-radius: 1px;
    border: 1px solid transparent;
}

QToolButton#buttonClose::separator {
    width: 1px;
    background: #a0555555;
}

QToolButton#buttonClose:hover {
    background: #a0444455;
    border: 0;
}

QToolButton#buttonClose:pressed {
    background: #a0444466;
    border: 0;
}

