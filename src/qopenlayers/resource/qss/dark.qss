/** QMenuBar **/

QMenuBar {
    background: #252526;
}

QMenuBar::item {
    padding: 4px 6px;
    border-radius: 1px;
}

QMenuBar::item:hover {
    background: #444;
}

QMenuBar::item:selected {
    background: #444;
}

/** QMenu **/

QMenu {
    background: #202020;
    margin: 1px;
}

QMenu::item {
    padding: 4px 30px 4px 25px;
    border: 1px solid transparent;
}

QMenu::item:selected {
    background: #444;
}

QMenu::item:disabled {
    background: #202020;
    color: #777;
}

QMenu::icon {
    left: 3px;
}

QMenu::icon:checked {
    background: gray;
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}

QMenu::separator {
    height: 1px;
    background: #555;
}

QMenu::indicator {
    width: 13px;
    height: 18px;
}
