
/** QWidget **/

QWidget {
    color: #fff;
}

QScrollArea[frameShape="6"], QFrame[frameShape="6"], QWidget[__pane__="true"] {
    background: #252526;
}

QWidget[__node__="true"] {
    background: rgba(80, 80, 100, 100);
    border-radius: 3px;
}

QDialog, QStackedWidget, QSplitter {
    background: #353535;
}

/** QMessageBox **/

QMessageBox {
    background: #353535;
    color: #ccc;
}

/** QMainWindow **/

QMainWindow {
    background: #333;
}

QMainWindow::separator {
    background: #444;
    width: 6px;
    height: 6px;
}

QMainWindow::separator:hover {
    background: #666;
}

/** QProgressBar **/

QProgressBar {
    background: #444;
    border: 1px solid #544;
    border-radius: 2px;
    text-align: center;
}

QProgressBar::chunk {
    background: green;
    width: 20px;
}

/** QSlider **/

QSlider::horizontal {
    height: 14px;
}

QSlider::groove:horizontal {
    height: 4px;
    border-radius: 2px;
    border: 1px solid #666;
    background: #556;
}

QSlider::handle:horizontal {
    background: rgb(0, 160, 230);
    width: 14px;
    border-radius: 6px;
    margin: -5px 0;
}

QSlider::vertical {
    width: 14px;
}

QSlider::groove:vertical {
    width: 4px;
    border-radius: 2px;
    border: 1px solid #666;
    background: #556;
}

QSlider::handle:vertical {
    background: rgb(0, 160, 230);
    width: 14px;
    border-radius: 6px;
    margin: -5px 0;
}

/** QTabWidget **/

QTabWidget {
    background: #1e1e1e;
}

QTabWidget::pane {
    border: 0px solid #3a3a3a;
    position: absolute;
    top: 0.0em;
    background: #262626;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar::tab {
    background: #2c2c2c;
    min-width: 8ex;
    padding: 6px;
    padding-left: 12px;
    padding-right: 12px;
    color: #999;
}

QTabWidget QTabBar QToolButton {
    background: #383838;
    border: none;
}

QTabWidget QTabBar QToolButton:hover {
    background: #444455;
}

QTabWidget QTabBar QToolButton:pressed {
    background: #444466;
}

QTabWidget QTabBar QToolButton:disabled {
    background: #222;
}

/** QTabBar **/

QTabBar {
    background: transparent;
    outline: none;
}

QTabBar::tab:selected {
    background: #246;
    color: lightgreen;
}

QTabBar::tab:hover {
    background: #347;
}

/** QDockWidget **/

QDockWidget {
    background: #1e1e1e;
}

QDockWidget::title {
    background: #3f2a2a;
    color: #ccc;
    /*border-top-left-radius: 3px;
    border-top-right-radius: 3px;*/
    text-align: left;
    padding-left: 3px;
    padding-top: 4px;
    padding-bottom: 4px;
}

QDockWidget::close-button, QDockWidget::float-button {
    background: #55555a;
    padding: 0px;
    icon-size: 20px;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    background: darkgray;
    border: 0px;
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    padding: 1px -1px -1px 1px;
}

QDockWidget::close-button {
    subcontrol-position: center right;
    subcontrol-origin: margin;
    position: absolute;
    right: 5px;
    width: 26px;
    min-height: 21px;
}

QDockWidget::float-button {
    subcontrol-position: center right;
    subcontrol-origin: margin;
    position: absolute;
    right: 33px;
    width: 26px;
    min-height: 21px;
}

QDockWidget QTabBar::tab {
    padding: 8px;
    padding-left: 6px;
    padding-right: 6px;
}
